from package.geometric import reactangleArea,circle
from package.arithmatic import addition,mul,divi,subtract

print("area of a ractangle is : ",reactangleArea(3,5))
print("area of a circle is : ", circle(2))

print("addition : ",addition(3,5))
print("addition : ",mul(3,5))
print("addition : ",divi(6,3))
print("addition : ",subtract(6,3))

